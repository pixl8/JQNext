{
  "name": "jqnext",
  "version": "1.0.7",
  "description": "Modern jQuery 2.x API-compatible library using native JavaScript internals - drop-in replacement for jQuery with jQuery UI support",
  "main": "dist/jqnext.js",
  "module": "dist/jqnext.esm.js",
  "types": "dist/jqnext.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c && npm run copy-to-test",
    "copy-to-test": "cp dist/jqnext.js test/lib/jqnext.js && cp dist/jqnext.js.map test/lib/jqnext.js.map",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "jquery",
    "jquery-alternative",
    "jquery-replacement",
    "jquery-ui",
    "dom",
    "ajax",
    "events",
    "animation",
    "modern",
    "es6",
    "vanilla-js",
    "web-animations",
    "fetch-api"
  ],
  "author": "Pixl8 Group Limited",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ready-intelligence/jqnext"
  },
  "bugs": {
    "url": "https://gitlab.com/ready-intelligence/jqnext/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
